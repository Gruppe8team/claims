<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="claims.controllers.Admin.AdminController"
            prefWidth="600" prefHeight="400">

    <top>
        <Label text="ADMINISTRATOR VIEW" />
    </top>

    <left>
        <Button text="CLAIMS" fx:id="claimsBtn" />
    </left>

    <center>
        <Button text="CUSTOMERS" fx:id="customersBtn" />
    </center>

    <right>
        <Button text="POLICIES" fx:id="policiesBtn" />
    </right>

    <bottom>
        <Button text="ADVISORS" fx:id="advisorsBtn" />
    </bottom>

</BorderPane>
